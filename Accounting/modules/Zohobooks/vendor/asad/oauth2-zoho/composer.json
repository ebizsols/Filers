{
    "name": "asad/oauth2-zoho",
    "description": "Zoho OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "zoho"
    ],
    "homepage": "https://github.com/asad/oauth2-zoho",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Asadur Rahman",
            "email": "asadku34@gamil.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "eloquent/phony-phpunit": "^2.0",
        "phpunit/phpunit": "^6.0",
        "mockery/mockery": "~0.9",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "php-coveralls/php-coveralls": "^2.1",
        "squizlabs/php_codesniffer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Asad\\OAuth2\\Client\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asad\\OAuth2\\Client\\Test\\": "test/src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check": "phpcs src --standard=psr2 -sp"
    },
    "config": {
        "sort-packages": true
    }
}
